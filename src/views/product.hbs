<div>
    <h1>Listado de Productos</h1>
</div>
<div>
    <h2>MongoDB Atlas</h2>
    <table class="uk-table uk-table-striped uk-table-hover">
        <thead>
            <tr>
                <th>Código</th>
                <th>Título</th>
                <th>Descripción</th>
                <th>Precio</th>
                <th>Stock</th>
            </tr>
        </thead>
        <tbody>
            {{#each prodList}}
                <tr>
                    <td>{{this.code}}</td>
                    <td>{{this.title}}</td>
                    <td>{{this.description}}</td>
                    <td>${{this.price}}</td>
                    <td>{{this.stock}}</td>
                </tr>
            {{/each}}
        </tbody>
    </table>
</div>